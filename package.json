{
  "name": "sunrize",
  "productName": "Sunrize X3D Editor",
  "version": "1.2.5",
  "description": "A Multi-Platform X3D Editor",
  "main": "src/main.js",
  "bin": {
    "sunrize": "bin/sunrize.js"
  },
  "files": [
    "bin/*",
    "src/*"
  ],
  "scripts": {
    "start": "electron .",
    "development": "SUNRISE_ENVIRONMENT=DEVELOPMENT electron .",
    "package": "electron-forge package",
    "premake": "npm i -D electron",
    "make": "electron-forge make",
    "postmake": "npm i -P electron",
    "publish": "node build/publish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/create3000/sunrize.git"
  },
  "keywords": [
    "3D",
    "editor",
    "electron",
    "GLB",
    "glTF2",
    "JavaScript",
    "JSON",
    "node",
    "npx",
    "PLY",
    "STL",
    "SVG",
    "VRML",
    "Wavefront-OBJ",
    "WebGL",
    "wrl",
    "x_ite",
    "X3D",
    "XML"
  ],
  "author": "Holger Seelig",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/create3000/sunrize/issues"
  },
  "homepage": "https://create3000.github.io/sunrize/",
  "contributors": [
    {
      "name": "Holger Seelig",
      "email": "holger.seelig@gmail.com",
      "url": "https://github.com/create3000"
    }
  ],
  "devDependencies": {
    "@electron-forge/cli": "^7.1.0",
    "@electron-forge/maker-deb": "^7.1.0",
    "@electron-forge/maker-rpm": "^7.1.0",
    "@electron-forge/maker-squirrel": "^7.1.0",
    "@electron-forge/maker-zip": "^7.1.0"
  },
  "dependencies": {
    "capitalize": "^2.0.4",
    "console": "^0.7.2",
    "electron": "^27.1.2",
    "electron-prompt": "^1.7.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-tabs": "^1.0.2",
    "fullname": "^5.0.0",
    "jquery": "^3.7.1",
    "jquery-ui-dist": "^1.13.2",
    "jstree": "^3.3.16",
    "material-icons": "^1.13.12",
    "material-symbols": "^0.14.1",
    "md5": "^2.3.0",
    "monaco-editor": "^0.44.0",
    "node-clipboardy": "^1.0.3",
    "node-localstorage": "^3.0.5",
    "qtip2": "^3.0.3",
    "spectrum-colorpicker2": "^2.0.10",
    "string-similarity": "^4.0.4",
    "x_ite": "^9.1.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/images/icon.png",
        "extendInfo": "./src/assets/Info.plist"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Sunrize"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
